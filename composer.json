{
	"name": "phpstan/phpstan-guzzle",
	"description": "Guzzle extensions for PHPStan",
	"license": ["MIT"],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "~7.0",
		"phpstan/phpstan": "^0.6.3",
		"guzzlehttp/guzzle": "~6.0"
	},
	"require-dev": {
		"consistence/coding-standard": "~0.12.0",
		"phing/phing": "^2.13.0",
		"phpunit/phpunit": "^5.7|^6.0",
		"satooshi/php-coveralls": "dev-master",
		"slevomat/coding-standard": "^1.1.1"
	},
	"autoload": {
		"psr-4": {
			"PHPStan\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	}
}
